									"\r",
									"var response = pm.response.json();\r",
									"\r",
									"// Check if login was successful and token exists\r",
									"if (response.status && response.data && response.data.token) {\r",
									"    // Save token to environment variable\r",
									"    pm.environment.set(\"token\", response.data.token);\r",
									"    console.log(\"✅ Token saved to environment variable: token\");\r",
									"} else {\r",
